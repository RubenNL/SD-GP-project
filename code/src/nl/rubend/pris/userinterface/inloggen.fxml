<AnchorPane prefHeight="309.0" prefWidth="423.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.rubend.pris.userinterface.Inloggen">
   <children>
      <GridPane layoutX="52.0" layoutY="158.0" prefHeight="86.0" prefWidth="339.0">
        <columnConstraints>
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="162.0" minWidth="10.0" prefWidth="98.0" />
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="246.0" minWidth="10.0" prefWidth="241.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="45.0" minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="42.0" minHeight="10.0" prefHeight="41.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="E-mail" />
            <Label text="Wachtwoord" GridPane.rowIndex="1" />
            <TextField fx:id="emailBox" GridPane.columnIndex="1" />
            <PasswordField fx:id="wachtwoordBox" GridPane.columnIndex="1" GridPane.rowIndex="1" />
