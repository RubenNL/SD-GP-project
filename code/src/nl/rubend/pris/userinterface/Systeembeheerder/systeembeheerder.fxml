                        <Label text="Naam" textAlignment="CENTER" />
                        <Label text="E-mail " textAlignment="CENTER" />
                        <Label text="Wachtwoord" textAlignment="CENTER" />
                        <Label fx:id="nummberLabel" text="nummer" textAlignment="CENTER" />
                        <Label fx:id="hoortBijLabel" text="" textAlignment="CENTER" />
                     </children>
                  </VBox>
                  <Button layoutX="313.0" layoutY="346.0" mnemonicParsing="false" onAction="#maakAccountAan" prefHeight="26.0" prefWidth="81.0" text="Ok" />
                  <Button layoutX="309.0" layoutY="53.0" mnemonicParsing="false" onAction="#handleUitloggen" prefHeight="26.0" prefWidth="81.0" text="Uitloggen" />
