                  <TextField fx:id="nieuwAccountEmail" layoutX="178.0" layoutY="154.0" />
                  <TextField fx:id="nieuwAccountWachtwoord" layoutX="178.0" layoutY="193.0" />
                  <TextField fx:id="nieuwAccountNummer" layoutX="178.0" layoutY="231.0" />
