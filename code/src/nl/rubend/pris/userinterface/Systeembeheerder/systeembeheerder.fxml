                  <ComboBox fx:id="accoutsTypeComboBox" layoutX="14.0" layoutY="14.0" prefHeight="26.0" prefWidth="124.0" />
                  <TableView fx:id="accountsTabel" layoutX="-4.0" layoutY="63.0" prefHeight="277.0" prefWidth="395.0">
