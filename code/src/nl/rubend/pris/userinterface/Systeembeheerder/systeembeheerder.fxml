                        <TableColumn prefWidth="81.0" text="wachtwoord" />
                        <TableColumn minWidth="0.0" prefWidth="33.0" text="verwijder" />
                    </columns>
                  </TableView>
                  <ScrollBar layoutX="-4.0" layoutY="340.0" prefHeight="17.0" prefWidth="395.0" />
                  <ScrollBar layoutX="386.0" layoutY="63.0" orientation="VERTICAL" prefHeight="277.0" prefWidth="17.0" />
                  <Button fx:id="uitloggenButton" layoutX="310.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleUitloggen" text="Uitloggen" />
               </children>
            </AnchorPane>
         </children>
      </GridPane>
   </children>
</AnchorPane>
