              </rowConstraints>
               <children>
                  <Button fx:id="accountAanmakenMenuButton" mnemonicParsing="false" onAction="#toonScherm" prefHeight="48.0" prefWidth="176.0" text="Accont aanmaken" />
                  <Button fx:id="accountWeergevenMenuButton" mnemonicParsing="false" onAction="#toonScherm" prefHeight="50.0" prefWidth="176.0" text="Accounts weergeven" GridPane.rowIndex="1" />
               </children>
            </GridPane>
         </children>
      </VBox>
      <GridPane layoutX="167.0" layoutY="14.0" prefHeight="400.0" prefWidth="444.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="141.0" minWidth="8.0" prefWidth="14.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="408.0" minWidth="10.0" prefWidth="408.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="115.0" minWidth="10.0" prefWidth="22.0" />
        </columnConstraints>
        <rowConstraints>
