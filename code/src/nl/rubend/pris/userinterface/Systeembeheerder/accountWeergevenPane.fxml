        <ComboBox fx:id="accountTypeComboBox" layoutX="5.0" layoutY="14.0" onAction="#handleComboBoxViewItemsInList" prefHeight="26.0" prefWidth="144.0" />
        <TableView fx:id="tableView" layoutX="5.0" layoutY="63.0" prefHeight="266.0" prefWidth="594.0">
            <columns>
            <TableColumn fx:id="typeCol" prefWidth="121.0" text="Type" />
            <TableColumn fx:id="numCol" minWidth="2.0" prefWidth="108.0" text="Nummer" />
            <TableColumn fx:id="naamCol" minWidth="0.0" prefWidth="103.0" text="Naam" />
            <TableColumn fx:id="emailCol" prefWidth="151.0" text="E-mail" />
            </columns>
        </TableView>
      <Button layoutX="149.0" layoutY="350.0" mnemonicParsing="false" onAction="#handleNieuwWachtWoordOpstellen" text="Nieuw Wachtwoord" />
      <Button layoutX="317.0" layoutY="350.0" mnemonicParsing="false" onAction="#handleAccountVerwijderen" text="Vewijder Account" />
    </children>
</AnchorPane>
